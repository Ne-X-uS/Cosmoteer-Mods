ID = "kurim.enhancments"
Name = "Kurim's Enhancments"
Version = 1.0.0
Author = "Kurim"
CompatibleGameVersions = ["0.20.7", "0.20.9", "0.20.10", "0.20.11"]
Description = "Adds new Crew Quarters"
ModifiesMultiplayer = true
StringsFolder = "strings"
Logo = "logo.png"
Actions
[
	{
		Action = AddMany
		AddTo = "<ships/terran/terran.rules>/Terran/Parts"
		ManyToAdd = 
		[
			&<ships/crew_3x3/crew_3x3.rules>/Part
		]
	}
	{
		Action = AddMany
		AddTo = "<modes/career/techs.rules>/Techs"
		ManyToAdd = &<techs.rules>/Techs
	}
	{
		Action = Add
		AddTo = "<gui/game/designer/build_gui.rules>/EditorGroups"
		CreateIfNotExisting = false
		IgnoreIfNotExisting = false
		Name = Science
		ToAdd
		{
			NameKey = "EditorGroups/Science"
			Icon
			{
				Texture
				{
					File = "images/group_science.png"
					MipLevels = 2
					SampleMode = Linear
				}
			}
		}
	}
]